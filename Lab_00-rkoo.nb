 RowBox[{
  RowBox[{"v", "=", 
   RowBox[{"{", 
    RowBox[{"1", ",", "2"}], "}"}]}], ";", " ", 
  RowBox[{"u", "=", 
   RowBox[{"{", 
    RowBox[{"4", ",", "4"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"v", "+", "u"}], "\[IndentingNewLine]", 

Cell[BoxData[{
 RowBox[{
  RowBox[{"upper", "=", 
   RowBox[{"Plot3D", "[", 
    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{
       RowBox[{"x", "^", "2"}], "+", 
       RowBox[{"y", "^", "2"}]}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"y", ",", 
       RowBox[{"-", "1"}], ",", "1"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"lower", "=", 
   RowBox[{"Plot3D", "[", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{"Sqrt", "[", 
